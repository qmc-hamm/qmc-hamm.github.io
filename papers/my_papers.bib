@article{fumanalDiffusionMonteCarlo2016,
  title = {Diffusion {{Monte Carlo Perspective}} on the {{Spin-State Energetics}} of [{{Fe}}({{NCH}})6]2+},
  author = {Fumanal, Maria and Wagner, Lucas K. and Sanvito, Stefano and Droghetti, Andrea},
  year = {2016},
  month = sep,
  journal = {Journal of Chemical Theory and Computation},
  volume = {12},
  number = {9},
  pages = {4233--4241},
  issn = {1549-9618},
  doi = {10.1021/acs.jctc.6b00332},
  abstract = {The energy difference between the high spin and the low spin state of the model compound [Fe(NCH)6]2+ is investigated by means of Diffusion Monte Carlo (DMC), where special attention is dedicated to analyzing the effect of the fix node approximation on the accuracy of the results. For this purpose, we compare several Slater\textendash Jastrow and multireference Slater\textendash Jastrow trial wave functions. We found that a Slater\textendash Jastrow trial wave function constructed with the generalized Kohn\textendash Sham orbitals from hybrid DFT represents the optimal choice. This is understood by observing that hybrid functionals account for the subtle balance between exchange and correlation effects and the respective orbitals accurately describe the ligand\textendash metal hybridization as well as the charge reorganization accompanying the spin transition. Finally the DMC results are compared with those obtained by Hartree\textendash Fock, DFT, CASSCF, and CASPT2. While there is no clear reference value for the high spin\textendash low spin energy difference, DMC and high level CCSD(T) calculations agree within around 0.3 eV.},
  copyright = {All rights reserved},
  file = {/Users/lkwagner/Zotero/storage/N7HJA66P/Fumanal et al_2016_Diffusion Monte Carlo Perspective on the Spin-State Energetics of [Fe(NCH)6]2+.pdf}
}

