  journal = {arXiv:1505.03730 [cond-mat]},
  eprint = {1505.03730},
  eprinttype = {arxiv},
  primaryclass = {cond-mat},
  abstract = {Iron chalcogenides, FeTe and FeSe belong to the family of iron-based superconductors. We study magnetism of these compounds in the normal state, by using the ab initio downfolding scheme developed for strongly correlated electron systems. In deriving ab initio low-energy effective models, we employ the constrained GW method to eliminate the double counting of the electron correlations originating from the exchange correlations already taken into account in the density functional theory. By solving the derived ab initio effective models, we reveal that the elimination of the double counting is important in reproducing the bicollinear antiferromagnetic order in FeTe, as is observed in experiments. We also show that the elimination of the double counting induces a peculiar degeneracy of the several magnetic orders in FeSe. We discuss relation between the degeneracy and the recently found puzzling phenomena in FeSe.},
  archiveprefix = {arXiv},
  keywords = {Condensed Matter - Strongly Correlated Electrons},
  file = {/Users/lkwagner/Zotero/storage/J22Q4V65/Hirayama et al_2015_Ab initio Studies on Magnetism in Iron Chalcogenides FeTe and FeSe.pdf;/Users/lkwagner/Zotero/storage/3APH4WIX/1505.html}
}

