  file = {/Users/lkwagner/Zotero/storage/84PCAMDN/Jiang et al. - 2011 - Multireference Character for 3d Transition-Metal-C.pdf;/Users/lkwagner/Zotero/storage/ST8SMHT7/Jiang et al. - 2011 - Multireference Character for 3d Transition-Metal-C.pdf;/Users/lkwagner/Zotero/storage/5QB8DA4I/ct2006852.html;/Users/lkwagner/Zotero/storage/G6FMZZCP/Jiang et al. - 2011 - Multireference Character for 3d Transition-Metal-C.html;/Users/lkwagner/Zotero/storage/KC4SPC4Z/ct2006852.html;/Users/lkwagner/Zotero/storage/WM9JSQD3/ct2006852.html}
