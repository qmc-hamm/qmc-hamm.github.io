  journal = {arXiv:0902.1636},
  eprint = {0902.1636},
  eprinttype = {arxiv},
  doi = {10.1103/PhysRevLett.105.257001},
  abstract = {We determined electronic relaxation times via pump-probe optical spectroscopy using sub-15 fs pulses for the normal state of two different cuprate superconductors.We show that the primary relaxation process is the electron-phonon interaction and extract a measure of its strength, the second moment of the Eliashberg function\textbackslash lambda{$<\backslash$}omega\^\{2\}{$>$}=800\textbackslash pm200 meV\^\{2\} for La\_\{1.85\}Sr\_\{0.15\}CuO\_\{4\} and \textbackslash lambda{$<\backslash$}omega\^\{2\}{$>$}=400\textbackslash pm100 meV\^\{2\} for YBa\_\{2\}Cu\_\{3\}O\_\{6.5\}. These values suggest a possible fundamental role of the electron-phonon interaction in the superconducting pairing mechanism.},
  archiveprefix = {arXiv},
