@article{fournierUsingSwarmIntelligence2011,
  title = {Using Swarm Intelligence for Finding Transition States and Reaction Paths},
  author = {Fournier, Ren{\'e} and Bulusu, Satya and Chen, Stephen and Tung, Jamie},
  year = {2011},
  month = sep,
  journal = {The Journal of Chemical Physics},
  volume = {135},
  number = {10},
  pages = {104117},
  issn = {0021-9606},
  doi = {10.1063/1.3633515},
  abstract = {We describe an algorithm that explores potential energy surfaces (PES) and finds approximate reaction paths and transition states. A few ({$\approx$}6) evolving atomic configurations (``climbers'') start near a local minimum M1 of the PES. The climbers seek a shallow ascent, low energy, path toward a saddle point S12, cross over to another valley of the PES, and climb down to a new minimum M2 that was not known beforehand. Climbers use both energy and energy derivatives to make individual decisions, and they use relative fitness to make team-based decisions. In sufficiently long runs, they keep exploring and may go through a sequence M1\textendash S12\textendash M2\textendash S23\textendash M3 \ldots{} of minima and saddle points without revisiting any of the critical points. We report results on eight small test systems that highlight advantages and disadvantages of the method. We also investigated the PES of Li8, Al+7Al7+{$<$}math overflow="scroll" altimg="eq-00001.gif"{$><$}msubsup{$><$}mi{$>$} Al {$<$}/mi{$><$}mn{$>$}7{$<$}/mn{$><$}mo{$>$}+{$<$}/mo{$><$}/msubsup{$><$}/math{$>$}, Ag7, and Ag2NH3 to illustrate potential applications of this new method.},
  file = {/Users/lkwagner/Zotero/storage/DPF7AYAH/Fournier et al_2011_Using swarm intelligence for finding transition states and reaction paths.pdf;/Users/lkwagner/Zotero/storage/F455YSRG/1.html}
}

@article{foyevtsovaInitioQuantumMonte2014,
