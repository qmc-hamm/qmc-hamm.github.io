@article{leberreStructuralStudiesSingle2000,
  title = {Structural {{Studies}} on {{Single Crystals}} of {{Chevrel Phase Selenides REMo6Se8}} ({{RE}} = {{La}}, {{Ce}}, {{Pr}}, {{Nd}}, or {{Sm}}) at 298 {{K}}},
