  langid = {english},
  file = {/Users/lkwagner/Zotero/storage/VZKNCXMH/Ren et al_1998_Temperature-induced magnetization reversal in a YVO3 single crystal.pdf;/Users/lkwagner/Zotero/storage/S6QKC7KM/396441a0.html}
