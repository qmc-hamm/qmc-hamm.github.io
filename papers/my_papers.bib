@article{christleIsolatedSpinQubits2017,
  title = {Isolated {{Spin Qubits}} in {{SiC}} with a {{High-Fidelity Infrared Spin-to-Photon Interface}}},
  author = {Christle, David J. and Klimov, Paul V. and {de las Casas}, Charles F. and Sz{\'a}sz, Kriszti{\'a}n and Iv{\'a}dy, Viktor and Jokubavicius, Valdas and Ul Hassan, Jawad and Syv{\"a}j{\"a}rvi, Mikael and Koehl, William F. and Ohshima, Takeshi and Son, Nguyen T. and Janz{\'e}n, Erik and Gali, {\'A}d{\'a}m and Awschalom, David D.},
