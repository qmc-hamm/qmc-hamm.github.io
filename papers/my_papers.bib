@article{bianconiShapeResonancesMulticondensate2013,
  title = {Shape Resonances in Multi-Condensate Granular Superconductors Formed by Networks of Nanoscale-Striped Puddles},
  author = {Bianconi, Antonio},
  year = {2013},
  month = jul,
  journal = {Journal of Physics: Conference Series},
  volume = {449},
  number = {1},
