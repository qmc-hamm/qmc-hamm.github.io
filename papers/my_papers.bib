  file = {/Users/lkwagner/Zotero/storage/9WICYM8E/Kogar et al_2017_Signatures of exciton condensation in a transition metal dichalcogenide.pdf;/Users/lkwagner/Zotero/storage/2JH8P7CQ/1314.html}
