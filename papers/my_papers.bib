@article{gillanHighprecisionCalculationHartreeFock2008,
  title = {High-Precision Calculation of {{Hartree-Fock}} Energy of Crystals},
  author = {Gillan, M. J. and Alf{\`e}, D. and de Gironcoli, S. and Manby, F. R.},
  year = {2008},
  journal = {Journal of Computational Chemistry},
  volume = {29},
  number = {13},
  pages = {2098--2106},
  doi = {10.1002/jcc.21033},
  abstract = {When using quantum chemistry techniques to calculate the energetics of bulk crystals, there is a need to calculate the Hartree-Fock (HF) energy of the crystal at the basis-set limit. We describe a strategy for achieving this, which exploits the fact that the HF energy of crystals can now be calculated using pseudopotentials and plane-wave basis sets, an approach that permits basis-set convergence to arbitrary precision. The errors due to the use of pseudopotentials are then computed from the difference of all-electron and pseudopotential total energies of atomic clusters, extrapolated to the bulk-crystal limit. The strategy is tested for the case of the LiH crystal, and it is shown that the HF cohesive energy can be converged with respect to all technical parameters to a precision approaching 0.1 mEh per atom. This cohesive energy and the resulting HF value of the equilibrium lattice parameter are compared with literature values obtained using Gaussian basis sets. \textcopyright{} 2008 Wiley Periodicals, Inc. J Comput Chem, 2008}
}

