  archiveprefix = {arXiv},
  keywords = {Condensed Matter - Materials Science},
  file = {/Users/lkwagner/Zotero/storage/7Q2G3D8H/Muechler et al_2019_Influence of point defects on the electronic and topological properties of.pdf;/Users/lkwagner/Zotero/storage/E6Z6ZX2X/1911.html}
