@article{majzoubFirstPrinciplesCalculatedPhase2011,
  title = {First-{{Principles Calculated Phase Diagram}} for {{Nanoclusters}} in the {{Na}}-{{Al}}-{{H System}}: {{A Single-Step Decomposition Pathway}} for {{NaAlH4}}},
