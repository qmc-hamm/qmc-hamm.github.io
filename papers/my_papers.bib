  shorttitle = {Electronic Structure of an Antiferromagnetic Metal},
  author = {Bhobe, P. A. and Chainani, A. and Taguchi, M. and Eguchi, R. and Matsunami, M. and Ohtsuki, T. and Ishizaka, K. and Okawa, M. and Oura, M. and Senba, Y. and Ohashi, H. and Isobe, M. and Ueda, Y. and Shin, S.},
