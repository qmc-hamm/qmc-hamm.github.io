@article{alkauskasBandedgeProblemTheoretical2011,
  title = {Band-Edge Problem in the Theoretical Determination of Defect Energy Levels: {{The O}} Vacancy in {{ZnO}} as a Benchmark Case},
  shorttitle = {Band-Edge Problem in the Theoretical Determination of Defect Energy Levels},
  author = {Alkauskas, Audrius and Pasquarello, Alfredo},
  year = {2011},
  journal = {Physical Review B},
  volume = {84},
