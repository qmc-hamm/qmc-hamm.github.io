@article{alfeAnalyzingErrorsDFT2014,
  title = {Analyzing the Errors of {{DFT}} Approximations for Compressed Water Systems},
  author = {Alf{\`e}, D. and Bart{\'o}k, A. P. and Cs{\'a}nyi, G. and Gillan, M. J.},
