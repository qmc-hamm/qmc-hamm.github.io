  archiveprefix = {arXiv},
  keywords = {Computer Science - Machine Learning,Physics - Chemical Physics,Physics - Computational Physics},
  file = {/Users/lkwagner/Zotero/storage/Z9JSEHP6/Pfau et al_2020_Ab-Initio Solution of the Many-Electron Schr-odinger Equation with Deep Neural.pdf;/Users/lkwagner/Zotero/storage/QC76ME3R/1909.html}
