@article{imadaMetalinsulatorTransitions1998,
  title = {Metal-Insulator Transitions},
  author = {Imada, Masatoshi and Fujimori, Atsushi and Tokura, Yoshinori},
  year = {1998},
  month = oct,
  journal = {Reviews of Modern Physics},
  volume = {70},
