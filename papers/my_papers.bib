  file = {/Users/lkwagner/Zotero/storage/D5R89966/Vitali et al_2016_Computation of dynamical correlation functions for many-fermion systems with.pdf;/Users/lkwagner/Zotero/storage/J69Q3QBD/PhysRevB.94.html}
