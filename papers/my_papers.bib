  file = {/Users/lkwagner/Zotero/storage/PGM6UCDV/Zheng et al_2017_Stripe order in the underdoped region of the two-dimensional Hubbard model.pdf;/Users/lkwagner/Zotero/storage/8QQAI965/1155.html}
